.container {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  height: 100%;
}

.title {
  color: var(--black);
  margin-bottom: 20px;
  font-size: 40px;
  line-height: 40px;
  font-weight: 500;
}

.description {
  color: var(--black);
  margin-bottom: 70px;
  font-size: 20px;
  line-height: 20px;
}

.link {
  font-size: 11px;
  line-height: 11px;
}
